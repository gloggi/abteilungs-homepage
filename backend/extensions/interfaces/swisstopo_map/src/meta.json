{
  "name": "Swisstopo Map",
  "version": "1.0.0",
  "types": ["json"],
  "fieldset": true,
  "icon": "place",
  "options": {
    "height": {
      "name": "$t:height",
      "interface": "text-input",
      "default": 400
    },
    "mapLat": {
      "name": "$t:map_lat",
      "comment": "$t:map_lat_comment",
      "interface": "text-input",
      "default": 2683182
    },
    "mapLng": {
      "name": "$t:map_lng",
      "comment": "$t:map_lng_comment",
      "interface": "text-input",
      "default": 1248038
    },
    "defaultZoom": {
      "name": "$t:default_zoom",
      "comment": "$t:default_zoom_comment",
      "interface": "text-input",
      "default": 18
    },
    "maxZoom": {
      "name": "$t:max_zoom",
      "comment": "$t:max_zoom_comment",
      "interface": "text-input",
      "default": 28
    },
    "address_to_code": {
      "name": "$t:address_to_code",
      "comment": "$t:address_to_code_comment",
      "interface": "switch",
      "type": "BOOLEAN",
      "default": false
    }
  }
}